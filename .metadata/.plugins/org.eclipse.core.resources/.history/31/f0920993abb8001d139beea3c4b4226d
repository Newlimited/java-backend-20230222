package chapter3.B_VariableMethod;

class Triangle{
	double underSide;
	double height;
	double hypotenuse;
	
	double area;
	double circumference;
	
}
public class Method2 {

	public static void main(String[] args) {
		Triangle triangle1 = new Triangle();
		triangle1.underSide = 3;
		triangle1.height = 4;
		triangle1.hypotenuse = 5;
		
	}

}
